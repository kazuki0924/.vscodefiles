{
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "block-outline",
  "editor.fontFamily": "Cascadia Code PL",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.inlayHints.fontFamily": "Cascadia Code PL",
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "alt",
  "editor.renderWhitespace": "all",
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabSize": 2,
  "editor.wordBasedSuggestionsMode": "allDocuments",
  "editor.wordWrap": "on",
  "explorer.incrementalNaming": "smart",
  "explorer.sortOrder": "type",
  "explorer.sortOrderLexicographicOptions": "lower",
  "extensions.closeExtensionDetailsOnViewChange": false,
  "extensions.ignoreRecommendations": true,
  "files.autoGuessEncoding": true,
  "files.defaultLanguage": "${activeEditorLanguage}",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/node_modules": true
  },
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.fetchOnPull": true,
  "git.showPushSuccessNotification": true,
  "go.addTags": {
    "options": "json=omitempty",
    "promptForTags": false,
    "tags": "json",
    "template": "",
    "transform": "snakecase"
  },
  "go.alternateTools": {},
  "go.autocompleteUnimportedPackages": false,
  "go.buildFlags": [],
  "go.buildOnSave": "package",
  "go.buildTags": "",
  "go.coverMode": "default",
  "go.coverOnSave": true,
  "go.coverOnSingleTest": true,
  "go.coverOnSingleTestFile": true,
  "go.coverOnTestPackage": true,
  "go.coverShowCounts": false,
  "go.coverageDecorator": {
    "coveredBorderColor": "rgba(64,128,128,0.5)",
    "coveredGutterStyle": "blockblue",
    "coveredHighlightColor": "rgba(64,128,128,0.5)",
    "type": "highlight",
    "uncoveredBorderColor": "rgba(128,64,64,0.25)",
    "uncoveredGutterStyle": "slashyellow",
    "uncoveredHighlightColor": "rgba(128,64,64,0.25)"
  },
  "go.coverageOptions": "showBothCoveredAndUncoveredCode",
  "go.delveConfig": {
    "apiVersion": 2,
    "debugAdapter": "legacy",
    "dlvLoadConfig": {
      "followPointers": true,
      "maxArrayValues": 64,
      "maxStringLen": 64,
      "maxStructFields": -1,
      "maxVariableRecurse": 1
    },
    "showGlobalVariables": false,
    "substitutePath": []
  },
  "go.disableConcurrentTests": false,
  "go.docsTool": "godoc",
  "go.editorContextMenuCommands": {
    "addImport": true,
    "addTags": true,
    "benchmarkAtCursor": false,
    "debugTestAtCursor": true,
    "fillStruct": true,
    "generateTestForFile": false,
    "generateTestForFunction": true,
    "generateTestForPackage": false,
    "playground": true,
    "removeTags": true,
    "testAtCursor": true,
    "testCoverage": true,
    "testFile": true,
    "testPackage": false,
    "toggleTestFile": true
  },
  "go.enableCodeLens": {
    "references": true,
    "runtest": true
  },
  "go.formatFlags": [],
  "go.formatTool": "default",
  "go.generateTestsFlags": [],
  "go.gocodeAutoBuild": false,
  "go.gocodeFlags": [
    "-builtin",
    "-ignore-case",
    "-unimported-packages"
  ],
  "go.gocodePackageLookupMode": "go",
  "go.gopath": null,
  "go.goroot": null,
  "go.gotoSymbol.ignoreFolders": [],
  "go.gotoSymbol.includeGoroot": false,
  "go.gotoSymbol.includeImports": false,
  "go.inferGopath": false,
  "go.installDependenciesWhenBuilding": false,
  "go.languageServerExperimentalFeatures": {
    "diagnostics": true
  },
  "go.languageServerFlags": [],
  "go.lintFlags": [],
  "go.lintOnSave": "package",
  "go.lintTool": "staticcheck",
  "go.liveErrors": {
    "delay": 500,
    "enabled": false
  },
  "go.logging.level": "error",
  "go.playground": {
    "openbrowser": true,
    "run": true,
    "share": true
  },
  "go.removeTags": {
    "options": "",
    "promptForTags": false,
    "tags": ""
  },
  "go.testEnvFile": null,
  "go.testEnvVars": {},
  "go.testFlags": null,
  "go.testOnSave": false,
  "go.testTags": null,
  "go.testTimeout": "30s",
  "go.toolsEnvVars": {},
  "go.toolsGopath": null,
  "go.toolsManagement.autoUpdate": false,
  "go.toolsManagement.checkForUpdates": "proxy",
  "go.trace.server": "off",
  "go.useCodeSnippetsOnFunctionSuggest": false,
  "go.useCodeSnippetsOnFunctionSuggestWithoutType": false,
  "go.useGoProxyToCheckForToolUpdates": true,
  "go.useLanguageServer": true,
  "go.vetFlags": [],
  "go.vetOnSave": "package",
  "gopls": {},
  "gruvboxConcoctis.selectionColor": "neutral",
  "gruvboxConcoctis.useBoldFont": false,
  "gruvboxConcoctis.useItalicFont": true,
  "gruvboxConcoctis.usePalette": "material",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "npm.enableRunFromFolder": true,
  "scm.defaultViewMode": "tree",
  "search.maintainFileSearchCache": true,
  "search.quickOpen.history.filterSortOrder": "recency",
  "search.searchEditor.reusePriorSearchConfiguration": true,
  "search.sortOrder": "type",
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "Alacritty.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "workbench.colorTheme": "gruvboxConcoctis dark hard",
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.pinnedTabSizing": "shrink",
  "workbench.editor.tabSizing": "shrink",
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.list.keyboardNavigation": "filter",
  "workbench.list.smoothScrolling": true
}
